      <div class="col-md-3">
        
      </div>
      <div class="col-md-9">
        <div class="row">
          <h3><%= Scorecard.model_name.human %>
          <%= 
            link_to(
              content_tag(
                :i, 
                '', 
                class: "fa fa-external-link"
              ),
              nil,
              remote: true,
              onclick: "$('.nav-wizard a[href=\"#scorecard\"]').tab('show');"
            )
          %>
          <strong id='preview-scorecard-name'></strong></h3>
          <hr>
          <p id='preview-scorecard-description'></p>
          
          <h3>Wicked Problem 
            <%= 
              link_to(
                content_tag(
