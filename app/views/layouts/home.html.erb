<!DOCTYPE html>
<html>
  <head>
    <title><%= application_title%></title>
    <%= csrf_meta_tags %> 
    <%= csp_meta_tag %> 
    <%= javascript_pack_tag 'application' %>
    <%= stylesheet_pack_tag 'application' %>
    <script src="https://kit.fontawesome.com/60b51f5021.js" crossorigin="anonymous"></script>
  </head>
  <body class="skin-wicked sidebar-mini">
    <div class="wrapper">
      <%= render partial: "layouts/header" %> 
      <div class="content-wrapper">
        <section class="content">
            <%= render 'layouts/shared/flash' %>
            <div class="home">
              <%= yield %>
            </div>  
        </section>
      </div>
      <%= render partial: 'layouts/footer' %>
    </div>
  </body>
</html>
