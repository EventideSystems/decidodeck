<p id="notice"><%= notice %></p>

<h1>Video Tutorials</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @video_tutorials.each do |video_tutorial| %>
      <tr>
        <td><%= link_to 'Show', video_tutorial %></td>
        <td><%= link_to 'Edit', edit_video_tutorial_path(video_tutorial) %></td>
        <td><%= link_to 'Destroy', video_tutorial, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

