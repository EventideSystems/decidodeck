<p id="notice"><%= notice %></p>

<h1>Listing Wicked Problems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wicked_problems.each do |wicked_problem| %>
      <tr>
        <td><%= link_to 'Show', wicked_problem %></td>
        <td><%= link_to 'Edit', edit_wicked_problem_path(wicked_problem) %></td>
        <td><%= link_to 'Destroy', wicked_problem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Problem', new_wicked_problem_path %>
