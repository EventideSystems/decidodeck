<p id="notice"><%= notice %></p>

<h1>Organisations Imports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organisations_imports.each do |organisations_import| %>
      <tr>
        <td><%= link_to 'Show', organisations_import %></td>
        <td><%= link_to 'Edit', edit_organisations_import_path(organisations_import) %></td>
        <td><%= link_to 'Destroy', organisations_import, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organisations Import', new_organisations_import_path %>
