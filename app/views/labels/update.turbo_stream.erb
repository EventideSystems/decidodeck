<%= turbo_stream.replace :stylesheet do %>
  <%= turbo_frame_tag :stylesheet do %>
    <%= render 'labels/stylesheet', labels: @all_labels %>
  <% end %>
<% end %>

<%= turbo_stream.replace @label do %>
  <style>
    <%= render partial: 'labels/label', formats: :css, locals: { label: @label } %>
  </style>
  <%= render partial: 'labels/label', locals: { label: @label } %>
<% end %>


